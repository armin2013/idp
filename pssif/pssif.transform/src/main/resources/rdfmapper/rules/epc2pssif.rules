@prefix pssif: <http://www.sfb768.tum.de/voc/pssif/ns#> .
@prefix epc: <http://www.sfb768.tum.de/voc/epc#> .
@prefix graph:  <http://www.sfb768.tum.de/voc/graph#> .
@prefix graphattr:  <http://www.sfb768.tum.de/voc/graph/Attr#> .
@prefix graphannot:  <http://www.sfb768.tum.de/voc/graph/Annot#> .

[EPC_Performs:
		(?in epc:performs ?out) 
		makeSkolem(?a, ?in, ?out)
		->
		(?s	rdf:Type pssif:Performs)
		(?s pssif:Source ?in)
		(?s pssif:Target ?out)
]

[EPC_Organization:
		(?s rdf:type epc:Organizational_Unit)
		(?s epc:name ?n)
		->
		(?s	rdf:Type pssif:Actor)
		(?s	pssif:Name ?n)
]

[EPC_Function: 	(?s rdf:type epc:Function)
				(?s epc:name ?n ) 
				makeSkolem(?a, ?gid, ?n)
				->
				(?s rdf:type pssif:Activity )
				(?s	pssif:Name ?n )
]

		
[EPC_Event: (?s a epc:Event)
			(?s epc:name ?n ) 
			makeSkolem(?a, ?gid, ?n)
			->
			(?a rdf:type pssif:State )
			(?a	pssif:Name ?n )
]
		
[EPC_ControlFlow: 	(?s epc:flow ?t)
				makeSkolem(?c, ?s, ?t)
				->
				(?c rdf:type pssif:ControlFlow )
				(?c	pssif:Source ?s )
				(?c	pssif:Target ?t )
]

[EPC_Performs: 	(?s epc:performs ?t)
				makeSkolem(?c, ?s, ?t)
				->
				(?c rdf:type pssif:performs )
				(?c	pssif:Source ?s )
				(?c	pssif:Target ?t )
]